{
    "name": "urben-service-app",
    "version": "1.0.0",
    "main": "dist/server.js",
    "license": "MIT",
    "scripts": {
        "build": "esbuild api/index.ts --bundle --platform=node --target=node18 --outfile=dist/api/index.js --external:aws-sdk --external:mock-aws-s3 --external:nock --loader:.html=text",
        "typecheck": "tsc --noEmit",
        "start": "node dist/api/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
        "build:tsc": "tsc -p tsconfig.json"
    },
    "dependencies": {
        "bcrypt": "^5.1.0",
        "serverless-http": "^2.7.0",
        "nodemailer": "^6.9.3",
        "cloudinary": "^1.34.0",
        "dotenv": "^16.0.0",
        "express": "^4.18.2",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^7.0.0",
        "multer": "^1.4.5-lts.1",
        "node-cron": "^3.0.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "nodemon": "^2.0.22",
        "typescript": "^5.1.6",
        "ts-node-dev": "^2.0.0",
        "esbuild": "^0.19.4",
        "@types/express": "^4.17.17",
        "@types/node": "^20.4.2",
        "@types/mongoose": "^5.11.97",
        "@types/nodemailer": "^6.4.7",
        "@types/node-cron": "^2.0.3",
        "@types/bcrypt": "^5.0.0",
        "@types/multer": "^1.4.7"
    }
}